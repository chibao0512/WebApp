@model IEnumerable<ApplicationUser>

@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<a class="btn btn-primary" asp-action="RegisterOwner"><h3>Create Owner New</h3></a>
<table class="table table-hover">
    <tr>
        <th>
            ID
        </th>
        <th>
            User Name
        </th>
        <th>
            Email
        </th>

        <th>
            Phone
        </th>

        <th>

        </th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.Acc_Name</td>
            <td>@user.Email</td>

            <td>@user.PhoneNumber</td>


            <td>
                <a asp-controller="Admin" asp-action="EditOwner" asp-route-id="@user.Id">Edit</a>
                @*<a asp-action="Delete" asp-route-id="@user.Id" onclick="return confirm ('Are you sure?')">Delete</a>*@
                @* <a asp-action="AddToCart" asp-route-productid="@book.book_Id">Add To Cart</a>*@
            </td>
        </tr>
    }
</table>